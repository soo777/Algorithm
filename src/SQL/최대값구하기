# 프로그래머스 최대값 구하기

1. 정렬
SELECT DATETIME AS '시간' FROM ANIMAL_INS ORDER BY DATETIME DESC LIMIT 1;

2. 정렬
SELECT DATETIME AS '시간' FROM ANIMAL_INS ORDER BY DATETIME ASC LIMIT 1;

3. count
SELECT COUNT(*) AS COUNT FROM ANIMAL_INS;

4. count 중복 제거
SELECT COUNT(DISTINCT NAME) FROM ANIMAL_INS;

5. group by
SELECT ANIMAL_TYPE, COUNT(ANIMAL_TYPE)  FROM ANIMAL_INS WHERE ANIMAL_TYPE IN ('CAT', 'DOG') GROUP BY ANIMAL_TYPE ORDER BY ANIMAL_TYPE ASC;



